{
  "name": "ishop.web",
  "version": "1.0.0",
  "description": "ishop web",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run start-backend & npm run start-frontend",
    "backend": "cd .. && bundle exec unicorn -p 3000",
    "frontend": "webpack-dev-server --content-base src/admin",
